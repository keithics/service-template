{
  "name": "service-template",
  "version": "2.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "build": "NODE_PATH=./src tsc",
    "start": "node dist/index.js",
    "test": "NODE_PATH=./src jest --coverage --verbose",
    "dev": "NODE_PATH=./src ts-node-dev --respawn --clear --transpile-only --rs src/index.ts",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx && npx prettier --check ./src && tsc --noEmit",
    "pretty": "npx prettier --write ./src",
    "prepare": "node -e \"try { require('husky').install() } catch (e) {if (e.code !== 'MODULE_NOT_FOUND') throw e}\""
  },
  "dependencies": {
    "@signalyticsai/auth": "^4.1.2",
    "@signalyticsai/core": "^4.0.4",
    "@signalyticsai/errors": "^3.0.14",
    "@signalyticsai/joi": "^2.0.9",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "~4.18.1",
    "helmet": "^5.1.0",
    "joi": "^17.6.0",
    "lodash.clone": "^4.5.0",
    "lodash.foreach": "^4.5.0",
    "mongoose-paginate-v2": "^1.7.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "codelyzer": "^6.0.2",
    "debug": "^4.3.4",
    "eslint": "^8.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "pretty-format": "^28.1.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "4.7.4"
  }
}
